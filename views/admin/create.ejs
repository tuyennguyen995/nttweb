    <script type="text/javascript">
      function checkPw(form) {
      user = form.us.value;
      pw = form.pw.value;
      pw1 = form.pws.value;

      if(user==""){
        alert ("\nTên đăng nhập không được để trống!");
        return false;
      }
      else if (pw == "") {
        alert ("\nMật khẩu không được để trống!");
        return false;
      }else if (pw1 == "") {
        alert ("\nHãy nhập lại mật khẩu!");
        return false;
      } else if(pw1 != pw) {
        alert ("\nNhập lại mật khẩu chưa đúng!");
        return false;
      }else {
        return true;
      }
    }
    </script>
