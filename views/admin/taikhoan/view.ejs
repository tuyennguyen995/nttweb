<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Chi tiết tài khoản</h1>
                <form class="form-horizontal" action="" method="POST" enctype="application/x-www-form-urlencoded" onsubmit="return checkPw(this);">
                  <div class="row">
                    <div class=" col-lg-12">
                      <div class=" panel panel-default">
                        <div class="panel-heading">
                            Nhập thông tin Cá nhân
                        </div>
                        <div class="panel-body" >
                          <div class="row">
                            <div class="form-group col-md-4" style="margin-left: 5px;">
                                <label>Họ tên *</label>
                                <input readonly class="form-control" value="<%= data1[0][0].HOTEN%>" name="hoten" type="text" autofocus autocomplete="off">
                            </div>
                            <div class="form-group col-md-4" style="margin-left: 5px;">
                              <label>Giới Tính *</label>
                              <select class="form-control" name="gioitinh" disabled>
                                <option value="<%= data1[0][0].GIOITINH%>"><%= data1[0][0].GIOITINH%></option>
                              </select>
                            </div>
                            <div class="form-group col-md-4" style="margin-left: 5px;">
                              <label>Ngày sinh *</label>
                              <input readonly class="form-control" value="<%= ns[0][0].to_char%>"  name="ngaysinh" type="date" autofocus autocomplete="off">
                            </div>
                            <div class="form-group col-md-4" style="margin-left: 5px;">
                              <label>Số CMND *</label>
                              <input readonly class="form-control" value="<%= data1[0][0].CMND%>" name="cmnd" type="text" autofocus autocomplete="off">
                            </div>
                            <div class="form-group col-md-4" style="margin-left: 5px;">
                              <label>Ngày cấp *</label>
                              <input readonly class="form-control" value="<%= nc[0][0].to_char%>" name="ngaycap" type="date" autofocus autocomplete="off">
                            </div>
                            <div class="form-group col-md-4" style="margin-left: 5px;">
                              <label>Nơi cấp *</label>
                              <input readonly class="form-control" value="<%= data1[0][0].NOICAP%>" name="noicap" type="rext" autofocus autocomplete="off">
                            </div>
                            <div class="form-group col-md-4" style="margin-left: 5px;">
                              <label>Số điện thoại *</label>
                              <input readonly class="form-control" value="<%= data1[0][0].SDT%>" name="sdt" type="text" autofocus autocomplete="off">
                            </div>
                            <div class="form-group col-md-4" style="margin-left: 5px;">
                              <label>E-Mail</label>
                              <input readonly class="form-control" value="<%= data1[0][0].MAIL%>" name="mail" type="email" autofocus autocomplete="off">
                            </div>
                          </div>
                        </div>
                      </div>
                      <!-- Close panel -->
                    </div>
                    <div class="col-md-12">
                      <div class="panel panel-default">
                        <div class="panel-heading">
                            Nhập thông tin Cơ quan
                        </div>
                        <div class="panel-body" >
                          <div class="row">
                              <div class="form-group col-md-4" style="margin-left: 5px;">
                                <label>Mã Cơ quan</label>
                                <input readonly class="form-control" value="<%= data1[0][0].MA_COQUAN%>" name="macoquan" type="text" autofocus autocomplete="off">
                              </div>
                              <div class="form-group col-md-4" style="margin-left: 5px;">
                                <label>Tên Cơ quan *</label>
                                <input readonly class="form-control" value="<%= data1[0][0].TEN_COQUAN%>" name="tencoquan" type="text" autofocus autocomplete="off">
                              </div>
                              <div class="form-group col-md-4" style="margin-left: 5px;">
                                <label>Bộ phận</label>
                                <input readonly class="form-control" value="<%= data1[0][0].BOPHAN%>" name="bophan" type="text" autofocus autocomplete="off">
                              </div>
                              <div class="form-group col-md-4" style="margin-left: 5px;">
                                <label for="tinhthanh">Tỉnh thành </label>
                                <select class="form-control" name="tinhthanh" disabled onchange="getSelect1();">
                                  <option value="<%= data1[0][0].ma_tinh%>"><%= data1[0][0].TEN_TINH%></option>
                                </select>
                              </div>
                              <div class="form-group col-md-4" style="margin-left: 5px;">
                                <div id="list2" onchange="getSelect2();">
                                  <label for="quanhuyen">Quận/Huyện </label>
                                  <select class="form-control" name="" disabled>
                                    <option value="<%= data1[0][0].ma_quan_huyen%>"><%= data1[0][0].TEN_QUAN_HUYEN%></option>
                                  </select>
                                </div>
                              </div>
                              <div class="form-group col-md-4" style="margin-left: 5px;">
                                <div id="list3">
                                  <label for="phuongxa">Phường/Xã </label>
                                  <select class="form-control" name="phuongxa" disabled>
                                    <option value="<%= data1[0][0].ma_diachi%>"><%= data1[0][0].TEN_PHUONG_XA%></option>
                                  </select>
                                </div>
                              </div>
                              <div class="form-group col-md-4" style="margin-left: 5px;">
                                <label>Địa chỉ (Số/Đường/Khóm/Ấp) *</label>
                                <input readonly class="form-control" value="<%= data1[0][0].DIACHI_SO%>" name="diachi" type="text" autofocus autocomplete="off">
                              </div>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="col-md-12">
                      <div class="panel panel-default">
                        <div class="panel-heading">
                            Nhập thông tin Tài khoản
                        </div>
                        <div class="panel-body" >
                          <div class="row">
                              <div class="form-group col-md-4" style="margin-left: 5px;">
                                  <label>Tên Đăng nhập</label>
                                  <input readonly class="form-control" value="<%= data1[0][0].username%>" name="us" type="text" autofocus autocomplete="off">
                              </div>
                              <div class="form-group col-md-4" style="margin-left: 5px;">
                                <label>Mật khẩu</label>
                                <input class="form-control" value="<%= data1[0][0].password%>" name="pw" type="password" autofocus autocomplete="off" readonly>
                              </div>
                              <div class="form-group col-md-4" style="margin-left: 5px;">
                                <label>Xác nhận mật khẩu</label>
                                <input class="form-control" value="<%= data1[0][0].password%>" name="pws" type="password" autofocus autocomplete="off" readonly>
                              </div>
                              <div class="form-group col-md-4" style="margin-left: 5px;">
                                <label>Trạng thái</label>
                                <select class="form-control" name="tthai" disabled>
                                  <option value="<%= data1[0][0].trangthai%>"><%= data1[0][0].TEN_TRANGTHAI%></option>
                                </select>
                              </div>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="col-md-12">
                      <a style="float: right" href="/admin/taikhoan/edit/<%= data1[0][0].id%>" class="btn btn-primary" />Cập nhật</a>
                      <a style="float: right; margin-right: 4px; margin-bottom: 20px;" href="/admin/taikhoan" class="btn btn-default">Hủy</a>
                    </div>
                  </div>

                </form>
              </div>
                        <!-- /.col-lg-12 -->
            </div>
        </div>
                <!-- /.row -->
    </div>
            <!-- /.container-fluid -->
  </div>
        <!-- /#page-wrapper -->
</div>
    <!-- /#wrapper -->
